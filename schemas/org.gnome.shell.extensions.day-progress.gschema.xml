<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
  <schema id="org.gnome.shell.extensions.day-progress" path="/org/gnome/shell/extensions/day-progress/">
    <key name="show-indicator" type="b">
      <default>true</default>
    </key>
    <key name="show-elapsed" type="b">
      <default>false</default>
    </key>
    <key type="i" name="width">
        <default>15</default>
        <range min="1" max="50"/>
    </key>
    <key type="b" name="circular">
        <default>false</default>
    </key>
  </schema>
</schemalist>
